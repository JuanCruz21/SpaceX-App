---
import { getLaunchesLastest } from "../Services/SpaceXApi";
import Card from "./Card.astro";
const launches = await getLaunchesLastest();
---
<div class="grid gap-6 lg:grid-cols-3  sm:grid-cols-2">
    {launches.map((launch)=>
        <Card id={launch.id} name={launch.name} success={launch.success} Img={launch.links.patch.small} details={launch.details} flightNumber={launch.launchpad}/>
    )}
</div>